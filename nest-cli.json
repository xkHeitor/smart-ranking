{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": true,
  "projects": {
    "init": {
      "type": "application",
      "root": "apps/initial",
      "entryFile": "initial/src/main",
      "sourceRoot": "apps/initial/src",
      "compilerOptions": {
        "tsConfigPath": "apps/initial/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "api-gateway/src/main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "micro-admin-backend": {
      "type": "application",
      "root": "apps/micro-admin-backend",
      "entryFile": "micro-admin-backend/src/main",
      "sourceRoot": "apps/micro-admin-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micro-admin-backend/tsconfig.app.json"
      }
    }
  }
}
