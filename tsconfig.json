{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@configs": [
        "apps/common/src/configs"
      ],
      "@queue": [
        "apps/common/src/queue"
      ],
      "@initial": [
        "apps/initial/src"
      ],
      "@api-gateway": [
        "apps/api-gateway/src"
      ]
    }
  }
}